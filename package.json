  "devDependencies": {
    "@material-ui/codemod": "^1.1.0",
    "@storybook/react": "^3.2.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "brfs": "^1.4.3",
    "browserify": "^14.0.0",
    "browserify-hmr": "github:gmaclennan/browserify-hmr#gmaclennan-patch-1",
    "budo": "^10.0.3",
    "bundle-collapser": "^1.2.1",
    "copyfiles": "^2.0.0",
    "dependency-check": "^3.1.0",
    "dotenv": "^6.0.0",
    "envify": "^4.1.0",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "history": "^4.6.3",
    "mkdirp": "^0.5.1",
    "precinct": "^4.2.0",
    "pump": "^1.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "redbox-react": "^1.4.3",
    "request": "^2.79.0",
    "resolve": "^1.3.3",
    "rework": "^1.0.1",
    "rework-import": "^2.1.0",
    "rimraf": "^2.5.4",
    "standard": "^11.0.1",
    "storybook-addon-intl": "^2.1.2",
    "tape": "^4.7.0",
    "traverse": "^0.6.6",
    "uglify-es": "^3.1.3",
    "uglifyify": "^5.0.1",
    "urlencode": "^1.1.0"
  },
  "license": "MIT",
  "scripts": {
    "check": "npm run check:used && npm run check:unused",
    "check:used": "dependency-check ./package.json --detective precinct",
    "check:unused": "dependency-check ./package.json --no-dev --detective precinct --unused",
    "lint": "standard",
    "start": "budo example/index.js:bundle.js --pushstate --dir example -- -t babelify",
    "test": "exit 0",
    "build:example": "mkdirp build && npm run -s build:example:statics && npm run -s build:example:js",
    "build:example:statics": "copyfiles -u 1 'example/**/*' build",
    "build:example:js": "NODE_ENV=production browserify -t babelify -t brfs -g envify -g uglifyify example/index.js | uglifyjs -c comparisons=false -m > build/bundle.js",
    "build:markers": "node ./bin/build_markers.js",
    "build:es5": "rimraf es5 && babel src/ --source-maps -d es5/",
    "build:umd": "rimraf dist && mkdirp dist && NODE_ENV=production browserify -t babelify -g envify -g uglifyify -p bundle-collapser/plugin -s MapFilter src/index.js | uglifyjs -c warnings=false,comparisons=false -m > dist/react-mapfilter.js",
    "watch": "rimraf es5 && babel src/ -w --source-maps -d es5/",
    "prepublish": "npm run build:es5 & npm run build:umd",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build/**"
    ]
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/react-mapfilter.git"
  },
  "keywords": [
    "react",
    "geojson",
    "map",
    "filter"
  ],
  "author": "Digital Democracy",
  "bugs": {
    "url": "https://github.com/digidem/react-mapfilter/issues"
  },
  "homepage": "https://github.com/digidem/react-mapfilter#readme"
}
