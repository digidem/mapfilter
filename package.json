    "react-dom": "^16.9.0",
    "react-storybook-addon-props-combinations": "^1.1.0",
    "rimraf": "^2.7.1",
    "storybook-addon-intl": "^2.4.1",
    "storybook-addon-react-docgen": "^1.2.15",
    "tap": "^14.6.2",
    "tape": "^4.11.0"
