  "main": "es5/index.js",
  "peerDependencies": {
    "react": "^15.4.1 || ^16",
    "react-dom": "^15.4.1 || ^16",
    "@material-ui/core": "^1.5.1"
  },
  "dependencies": {
    "@mapbox/sexagesimal": "^1.0.0",
    "@material-ui/icons": "^1.1.1",
    "@segment/isodate": "^1.0.2",
    "autosuggest-highlight": "^3.1.0",
    "babel-runtime": "^6.20.0",
    "base64-url": "^2.2.0",
    "case": "^1.4.2",
    "classnames": "^2.2.5",
    "d3-dsv": "^1.0.7",
    "debug": "^3.1.0",
    "deep-equal": "^1.0.1",
    "dehumanize-date": "^2.6.2",
    "feature-filter-geojson": "^1.0.0",
    "file-saver": "^1.3.3",
    "flat": "^4.0.0",
    "geojson-flatten": "^0.2.1",
    "get-scrollbar-width": "^1.0.4",
    "insert-css": "^2.0.0",
    "is-buffer": "^2.0.3",
    "localforage": "^1.5.0",
    "lodash": "^4.16.4",
    "mapbox-gl": "^0.46.0",
    "memoize-one": "^5.0.0",
    "moment": "^2.15.2",
    "object-assign": "^4.1.0",
    "prop-types": "^15.5.10",
    "randombytes": "^2.0.5",
    "react-autosuggest": "^9.3.2",
    "react-date-range": "^0.9.4",
    "react-intl": "^2.3.0",
    "react-intl-redux": "^0.6.0",
    "react-isolated-scroll": "^0.1.1",
    "react-redux": "^5.0.5",
    "react-virtualized": "9.21.0",
    "recompose": "^0.27.1",
    "redux": "^3.7.1",
    "redux-persist": "^4.8.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1",
